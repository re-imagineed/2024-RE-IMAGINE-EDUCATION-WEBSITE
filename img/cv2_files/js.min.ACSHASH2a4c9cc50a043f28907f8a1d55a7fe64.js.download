                "playerTime": {
                    "playhead": 0,
                    "ts": +new Date()
                },
                "eventType": "adComplete",
                "qoeData": News.App.MediaAnalytics.qoeData(Player),
            }
        },
        adBreakFinished: function (Player) {
            return {
                "playerTime": {
                    "playhead": 0,
                    "ts": +new Date()
                },
                "eventType": "adBreakComplete",
                "qoeData": News.App.MediaAnalytics.qoeData(Player),
            }
        },

        qoeData: function (Player) {
            let metrics = {};
            if (Player) {
                metrics = Player.getPlayerMetrics();
            }
            if (!metrics) {
                console.debug("currentMetrics are unavailable for player")
            }
            if (metrics.videoBitrate === 0 || metrics.videoBitrate === undefined || metrics.videoBitrate === null) {
                metrics.videoBitrate === 0;
                if (window?.shakaPlayerAdapter?.bitrate?.getFirstSegmentBitrate()) {
                    // for HLS, Shaka player metrics does not have the videoBitrate. Instead, service worker will provide bitrate by parsing the manifest file.
                    metrics.videoBitrate = window.shakaPlayerAdapter.bitrate.getFirstSegmentBitrate();
                    console.debug('metrics.videoBitrate was 0, using shakaPlayerAdapter.bitrate.getFirstSegmentBitrate() value: ' + metrics.videoBitrate);
                } else if (metrics.streamBandwidth) {
                    metrics.videoBitrate =  metrics.streamBandwidth
                    console.debug('metrics.videoBitrate was 0, using metrics.streamBandwidth value: ' + metrics.videoBitrate);
                } else {
                    console.debug('metrics.videoBitrate was 0 and no other bitrate value was available for Adobe Media Analytics qoe');
                }
            }

            let framesPerSecond = Math.round(metrics.videoFrameRate) || 0,
                timeToStart = Math.round(metrics.liveLatency) || 0,
                droppedFrames = metrics.droppedFrames || 0
            return {
                "media.qoe.droppedFrames": droppedFrames, // value returned from player ex. 15
                "media.qoe.timeToStart": timeToStart, // value returned from player ex. 5
                "media.qoe.framesPerSecond": framesPerSecond, // value returned from player ex. 40
                "media.qoe.bitrate": metrics.videoBitrate, // value returned from player ex. 1600
            }
        },

        streamPingInterval: function (Player, location) {
            //set interval to track how long stream has been watched
            //store value in this.streamPlayhead
            if (!this.streamInterval) {

                this.streamInterval = setInterval(function () {
                    ++News.App.MediaAnalytics.STREAMPLAYHEAD;
                }, 1000);
            }
            if (!this.mediaInterval) {
                this.mediaInterval = setInterval(function () {
                    News.App.MediaAnalytics.triggerEvent(location, News.App.MediaAnalytics.mediaPing(Player));
                }, 10000);
            }
        },

        vodPingInterval: function (Player, location) {
            if (!this.mediaInterval) {
                this.mediaInterval = setInterval(function () {
                    News.App.MediaAnalytics.triggerEvent(location, News.App.MediaAnalytics.mediaPing(Player, Math.round(Player.lastCurrentTime)));
                }, 10000);
            }

        },

        pausePingInterval: function (Player, location) {
            if (!this.pauseInterval) {
                this.pauseInterval = setInterval(function () {
                    News.App.MediaAnalytics.triggerEvent(location, News.App.MediaAnalytics.mediaPing(Player, Math.round(Player.lastCurrentTime)));
                }, 10000);
            }
        },

        bufferingPingInterval: function (Player, location) {
            if (!this.bufferInterval) {
                this.bufferInterval = setInterval(function () {
                    News.App.MediaAnalytics.triggerEvent(location, News.App.MediaAnalytics.mediaPing(Player, Math.round(Player.lastCurrentTime)));
                }, 10000);
            }
        },

        adPing: function () {
            return {
                "playerTime": {
                    "playhead": 0,
                    "ts": +new Date()
                },
                "eventType": "ping"
            }
        },

        adPingInterval: function (location) {
            this.adInterval = setInterval(function () {
                News.App.MediaAnalytics.triggerEvent(location, News.App.MediaAnalytics.adPing())
            }, 1000);
        },

        clearAdPingInterval: function () {
            if (this.adInterval) {
                clearInterval(this.adInterval);
                this.adInterval = null;
            }
        },

        clearMediaPingInterval: function () {
            if (this.mediaInterval) {
                clearInterval(this.mediaInterval);
                this.mediaInterval = null;
            }
        },

        clearStreamPlayheadPingInterval: function () {
            if (this.streamInterval) {
                clearInterval(this.streamInterval);
                this.streamInterval = null;
            }
        },

        clearBufferPingInterval: function () {
            if (this.bufferInterval) {
                clearInterval(this.bufferInterval);
                this.bufferInterval = null;
            }
        },

        clearPausePingInterval: function () {
            if (this.pauseInterval) {
                clearInterval(this.pauseInterval);
                this.pauseInterval = null;
            }
        }

    }
})(jQuery);

$(function () {
   var pdfHostname = $('.body-container').data('pdfhost');
   var pdfPathname = $('.body-container').data('pdfpath');
   if (pdfPathname && pdfHostname) {
      $('.embedpdf').each(function (index, value) {
         $(this).hide();
         var currentElement = $(this);
         var pdfPath = $(this).find('iframe').attr('src');
         var pdfDocTitle = $(this).find('iframe').attr('alt');
         setTimeout(function () {
            if (pdfPath) {
               pdfPath = pdfPath.replace('#toolbar=0&navpanes=0', '');
               var serverUrl = pdfHostname + pdfPathname + "?file=" + pdfPath+"&title="+pdfDocTitle;
               var pdfElement = '<div class="embed-' + index + '" ><iframe src="' + serverUrl + '" allowfullscreen="allowfullscreen"></div>';
               currentElement.find('iframe').replaceWith(pdfElement);
            }
            currentElement.show();
         }, 500);
      });
   }

});

